{
    "name": "szemul/helper",
    "description": "Collection of different helpers",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "nesbot/carbon": "^2.52.0",
        "ext-iconv": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.8.2",
        "phpunit/phpunit": "^9.5.5"
    },
    "autoload": {
        "psr-4": {
            "Szemul\\Helper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Szemul\\Helper\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
